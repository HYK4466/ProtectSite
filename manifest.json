{
  "name": "Protect",
  "description": "More Security Layer for Certain Sites",
  "version": "1.0",
  "manifest_version": 3,

  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["storage", "tabs", "webNavigation"],
  "action": {
    "default_popup": "main.html",
    "default_title": "Protect!"
  }
}
